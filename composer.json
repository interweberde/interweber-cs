        "source": "https://github.com/interweberde/interweber-cs"
    },
    "require": {
        "php": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0.0",
        "cakephp/cakephp-codesniffer": "^5.1.1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
