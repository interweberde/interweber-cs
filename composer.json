{
    "version": "3.0.0",
    "name": "interweberde/interweber-cs",
    "description": "Interweber CodeSniffer Standards",
    "type": "phpcodesniffer-standard",
    "keywords": ["framework", "codesniffer"],
    "homepage": "https://interweber.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Melchior"
        }
    ],
    "support": {
